package activity;

import java.util.Scanner;

public class JavaApplication {
	public static void main(String args[]) {
		Scanner in = new Scanner(System.in);
		System.out.println("Which activity you want to execute?");
		int act = in.nextInt();
		switch(act) {
			case 1:{
				System.out.println("Hello\nAlexandra Abramov");
				break;
			} 
			case 2: {
				System.out.println("Sum is:"+sum(74, 36));
				break;
			}
			case 3:{
				System.out.println("Result is:"+divide(50, 3));
				break;
			}
			case 4:{
				
				System.out.println("Enter two number");
				int a = in.nextInt();
				int b = in.nextInt();
				System.out.println("Result is:"+multiply(a, b));
				break;
			}
				
		}
		
	}
	public static int sum(int a, int b) {
		return a+b;
	}
	public static int divide(int a, int b) {
		return a/b;
	}
	public static int multiply(int a, int b) {
		return a/b;
	}


}
